<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ReservationActivity"/>
        <activity android:name=".user.view.UserMainView" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".PlateActivity" />
        <activity android:name=".admin.view.AddPlateView" />
        <activity android:name=".UserActivity" />
        <activity android:name=".admin.view.AddAccountView" />
        <activity android:name=".admin.view.AccountDetailsView" />
        <activity android:name=".admin.view.MainView" />
        <activity android:name=".admin.view.LoginView" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>